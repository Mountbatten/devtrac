/**
 * Implements hook_menu_default_menu_custom().
 */
function devtrac7_profile_menu_menu_default_menu_custom() {
  $menus = array();

  $menus['devtrac7-profile-menu'] = array(
    'menu_name' => 'devtrac7-profile-menu',
    'title' => 'Devtrac7 Profile Menu',
    'description' => 'The menu in the right part of the toolbar containing links for the logged in user.',
  );

  return $menus;
}

